<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <a href="/event/newEvent">Novo Evento</a>
    <a href="/user/editUser">Editar Usu√°rio</a>

    <% if (existingEvents.length > 0) { %>
        <% existingEvents.forEach(event => { %>
            <div>
                <h2><%= event.date %></h2>
                <h3><%= event.description %></h3>

                <a href="bingoCard/newBingoCard?idEvent=<%= event.id %>">Nova cartela</a>
                <a href="/bingoCard?idEvent=<%= event.id  %>">Ver as cartelas</a>
                <a href="/event/editEvent?idEvent=<%= event.id %>">Editar evento</a>
            </div>
        <% }) %>     
    <% } else {%>
        <h3>Sem eventos</h3>
    <% } %>
</body>
</html>